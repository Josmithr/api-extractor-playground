{
	"name": "package-a",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/package-a-public.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./internal": {
			"import": {
				"types": "./dist/package-a-untrimmed.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"main": "dist/index.js",
	"types": "dist/package-a-public.d.ts",
	"scripts": {
		"build": "run-s build:tsc build:api-extractor",
		"build-clean": "run-s clean build",
		"build:api-extractor": "api-extractor run",
		"build:tsc": "tsc --pretty",
		"clean": "rimraf dist api-extractor-temp"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "~7.42.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"typescript": "^5.3.3"
	}
}
